{
  "name": "new-arch",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test --no-watch --source-map --browsers=ChromeHeadlessNoSandbox",
    "lint": "ng lint --max-warnings 0",
    "lint:fix": "ng lint --fix",
    "prepare": "husky"
  },
  "private": true,
  "type": "module",
  "lint-staged": {
    "*.{js,ts,html}": [
      "eslint"
